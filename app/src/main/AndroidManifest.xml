<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.huytran.entrancetest">
  <uses-permission android:name="android.permission.INTERNET"/>
  <application
    android:name="com.huytran.entrancetest.App"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
		android:usesCleartextTraffic="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.huytran.entrancetest.view.activities.SignUpActivity"
      android:label="WeWatch"
      android:screenOrientation="portrait"
      android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="com.huytran.entrancetest.view.activities.CategoryActivity"
        android:screenOrientation="portrait"/>

      <meta-data
          android:name="preloaded_fonts"
          android:resource="@array/preloaded_fonts" />
  </application>
</manifest>
